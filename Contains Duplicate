public class ContainDup {

    public static boolean same(int arr[]) {

        for (int i = 0; i <= arr.length - 1; i++) {

            for (int j = i + 1; j < arr.length; j++) {
                if (arr[i] == arr[j]) {
                    return true;
                }

            }
        }
        return false;
    }

    public static void main(String args[]) {
        int arr[] = { 2, 5, 6, 1 };
        System.out.println(same(arr));
    }

}
